{
    "name": "zivlify/zblogpackage",
    "description": "This is a blog package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "rajesh",
            "email": "rajesh@zivtrix.in"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Zivlify\\Zblog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zivlify\\Zblog\\Tests\\": "tests/",
            "Zivlify\\Zblog\\Models\\": "models/" 
        }
    },
    "extra" : {
        "provider": [
            "Zivlify\\Zblog\\ZblogServiceProvider"
        ]
    }

    
}
