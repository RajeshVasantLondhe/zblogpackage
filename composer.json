{
    "name": "zivlify/zblogpackage",
    "description": "This is a blog package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "rajesh",
            "email": "rajesh@zivtrix.in"
        }
    ],
    "require": {
        "php": "^7.3",
        "spatie/laravel-query-builder": "dev-master",
        "zivlify/core": "v1.0.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Zivlify\\Zblog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zivlify\\Zblog\\Tests\\": "tests/"
        }
    },
    "extra" : {
        "laravel": {
            "providers": [
                "Zivlify\\Zblog\\ZblogServiceProvider"
            ]
        }
    }, 
    "minimum-stability": "dev",
    "repositories":[
        {
            "type": "git",
            "url": "git@github.com:zivlify/core.git"
        }
    ]

}
